{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "essential": false,
      "hostname": "client",
      "image": "nailbiter/multi-client",
      "name": "client"
    },
    {
      "essential": false,
      "hostname": "api",
      "image": "nailbiter/multi-server",
      "name": "server"
    },
    {
      "essential": false,
      "hostname": "worker",
      "image": "nailbiter/multi-worker",
      "name": "worker"
    },
    {
      "essential": true,
      "hostname": "nginx",
      "image": "nailbiter/multi-nginx",
      "links": [
        "client",
        "server"
      ],
      "name": "nginx",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
